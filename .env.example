# Synthetic Data Generator Configuration
# Copy this file to .env and fill in your values

# Application Settings
APP_NAME="Synthetic Data Generator"
DEBUG=false
LOG_LEVEL=INFO

# CDP Connection Settings
CDP_HOST=localhost
CDP_PORT=21050
CDP_DATABASE=default
CDP_AUTH_MECHANISM=PLAIN
CDP_USE_SSL=true
CDP_USER=
CDP_PASSWORD=
CDP_CA_CERT=

# Iceberg Catalog Settings
ICEBERG_CATALOG_NAME=iceberg
ICEBERG_CATALOG_TYPE=hive
ICEBERG_WAREHOUSE=
ICEBERG_URI=

# Synthetic Data Generation Settings
SYNTH_DEFAULT_SAMPLE_SIZE=1000
SYNTH_MAX_SAMPLE_SIZE=100000
SYNTH_PRESERVE_CORRELATIONS=true
SYNTH_RANDOM_SEED=
